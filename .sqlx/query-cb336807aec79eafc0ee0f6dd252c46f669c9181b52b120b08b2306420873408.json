  "query": "\n            SELECT ak.id, ak.long_token_hash, ak.user_id, u.active FROM users u\n              JOIN api_keys ak ON ak.user_id = u.id\n            WHERE u.username = $1\n                AND ak.short_token = $2\n                AND u.role = 'user'\n            LIMIT 1\n            ",
