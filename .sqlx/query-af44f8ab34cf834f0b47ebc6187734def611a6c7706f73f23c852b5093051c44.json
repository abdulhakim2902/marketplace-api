  "db_name": "PostgreSQL",
  "query": "\n            SELECT \n                nfts.*,\n                CASE\n                    WHEN rarity IS NOT NULL\n                    THEN RANK () OVER (\n                        PARTITION BY collection_id\n                        ORDER BY rarity DESC\n                    )\n                END                                 AS ranking\n            FROM nfts\n            WHERE id = ANY($1)\n            ",
