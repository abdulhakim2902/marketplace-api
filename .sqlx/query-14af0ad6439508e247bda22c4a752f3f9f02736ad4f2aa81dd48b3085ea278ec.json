{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT \n                a.tx_type,\n                a.tx_index,\n                a.tx_id,\n                a.sender,\n                a.receiver,\n                a.price,\n                a.usd_price,\n                a.market_name,\n                a.market_contract_id,\n                a.nft_id,\n                a.collection_id,\n                a.block_time,\n                a.block_height,\n                a.amount\n            FROM activities a\n            WHERE ($1::TEXT IS NULL OR $1::TEXT = '' OR a.collection_id = $1)\n                AND ($2::TEXT IS NULL OR $2::TEXT = '' OR a.nft_id = $2)\n            ORDER BY a.block_time\n            LIMIT $3 OFFSET $4\n            ",
